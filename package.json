{
  "name": "git-x",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "repository": "git@github.com:futpib/git-x.git",
  "license": "MIT",
  "packageManager": "yarn@3.3.0",
  "scripts": {
    "build": "tsc",
    "test": "nyc ava",
    "dev": "tsc --watch"
