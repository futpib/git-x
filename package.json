    "git-xworktree": "./lib/cli.js"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^18.16.18",
    "ava": "^5.3.1",
    "nyc": "^15.1.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "commander": "^9.5.0",
    "execa": "^6.1.0",
    "minimatch": "^5.1.6"
  }
}
